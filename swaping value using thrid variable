import java.io.*;
public class swaping
{
    static void SwapValue(int m,int n)
    {
        int temp=m;
        m=n;
        n=temp;
        System.out.println("value of m is  " +m +" and the value of n is " + n);
    }
    public static void main(String[]args)
    {
        int m=9,n=5;
        SwapValue(m,n);

    }
}
